{
  "root": true,
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {"browser": true, "es2021": true},
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
